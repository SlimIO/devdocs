{
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "template": "node_modules/@escommunity/minami"
    },
    "source": {
        "include": [
            "public/js/home.js",
            "src/main.js",
            "src/utils.js",
            "src/test.js",
            "bin/index.js",
            "src/generator.class.js"
        ],
        "exclude": [
            "node_modules",
            "client"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}