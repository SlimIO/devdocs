{
    "name": "@slimio/documentation",
    "version": "0.1.0",
    "description": "SlimIO Documentation Generator (CLI)",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/documentation.git"
    },
    "keywords": [
        "SlimIO",
        "documentation"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/documentation/issues"
    },
    "homepage": "https://github.com/SlimIO/documentation#readme",
    "dependencies": {
        "@polka/send-type": "^0.5.2",
        "@slimio/arg-parser": "^0.3.1",
        "@slimio/jsdoc": "^0.2.0",
        "@slimio/manifest": "^0.4.0",
        "dotenv": "^8.0.0",
        "kleur": "^3.0.3",
        "polka": "^0.5.2",
        "serve-static": "^1.13.2",
        "zup": "0.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.6.1",
        "@commitlint/config-conventional": "^7.6.0",
        "@slimio/eslint-config": "^2.0.4",
        "@types/node": "^11.13.11",
        "ava": "^1.4.1",
        "eslint": "^5.16.0",
        "husky": "^2.2.0"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "bin": {
        "slimdoc": "./bin/index.js"
    }
}
