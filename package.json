{
    "name": "@slimio/documentation",
    "version": "0.1.1",
    "description": "SlimIO Documentation Generator (CLI)",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/documentation.git"
    },
    "keywords": [
        "SlimIO",
        "documentation"
    ],
    "files": [
        "bin/index.js",
        "src/*.js",
        "public"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/documentation/issues"
    },
    "homepage": "https://github.com/SlimIO/documentation#readme",
    "dependencies": {
        "@polka/send-type": "^0.5.2",
        "@slimio/arg-checker": "^0.1.1",
        "@slimio/arg-parser": "^0.3.2",
        "@slimio/is": "^1.5.1",
        "@slimio/jsdoc": "^0.4.0",
        "@slimio/manifest": "^0.7.1",
        "kleur": "^3.0.3",
        "make-promises-safe": "^5.0.0",
        "open": "^6.4.0",
        "polka": "^0.5.2",
        "semver": "^6.3.0",
        "sirv": "^0.4.2",
        "zup": "0.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@slimio/eslint-config": "^3.0.3",
        "@types/node": "^12.6.8",
        "ava": "^2.3.0",
        "cross-env": "^5.2.0",
        "eslint": "^6.2.2",
        "husky": "^3.0.2",
        "minami": "^1.2.3"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "bin": {
        "slimdoc": "./bin/index.js"
    }
}
